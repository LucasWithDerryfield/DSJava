public class App {
    public static void main(String[] args) throws Exception {
        double[] values = new double[] {2.2, 1.0, 3.7, 4.1, 88.0};
        int i = 1;
        int v = 0;
        while (i < 6) {
            System.out.println(values[v]);
            v = v + 1;
            i = i + 1;
        }
    }
}
